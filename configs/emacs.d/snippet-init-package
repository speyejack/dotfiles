;;; init-$1.el --- $2
;;; Commentary:

;;; Code:

(defun jag--set-$1-key-bindings ()
  "Set up keybindings for $1.")

(defun jag--setup-$1-config ()
  "Set up personal configuation for $1.")

(defun jag--load-$1-requires ()
  "Load required sub packages for $1."$3)

(use-package $1
  :ensure t
  :config
  (jag--load-$1-requires)
  (jag--set-$1-key-bindings)
  (jag--setup-$1-config))

(provide 'init-$1)
;;; init-$1.el ends here
